      <MedicalTendQuality>0.6</MedicalTendQuality>
      <MedicalSurgerySuccessChance>0.8</MedicalSurgerySuccessChance>
      <Mass>150.2</Mass>
      <ATR_MaintenanceRetention>1</ATR_MaintenanceRetention>
    </statBases>
    <tools>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>22</power>
        <cooldownTime>1.0</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>39</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>22</power>
        <cooldownTime>1.0</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>39</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>16</power>
        <cooldownTime>1.2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <leatherDef>Tainted_Skin</leatherDef>
      <body>ATR_AndroidBody</body>
      <baseHealthScale>0.8</baseHealthScale>
      <baseHungerRate>0.6</baseHungerRate>
      <baseBodySize>0.8</baseBodySize>
      <gestationPeriodDays>1</gestationPeriodDays>
      <specialShadowData>
        <volume>(0.3, 0.8, 0.4)</volume>
        <offset>(0,0,0)</offset>
      </specialShadowData>
      <ageGenerationCurve>
        <points>
          <li>(0,0)</li>
          <li>(10,100)</li>
          <li>(2,0)</li>
        </points>
      </ageGenerationCurve>
    </race>
    <recipes>
